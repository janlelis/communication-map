---
layout: false
---
<!doctype html>
<html lang="en" ng-app="cm">
  <head>
    <title ng-bind="title">Communication Map</title>
    <meta charset="utf-8">
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="keywords" content="palava, webrtc, video, chat, p2p">
    <meta name="description" content="WebRTC based P2P Network for Location Sharing">

    <script type="text/javascript" src="http://maps.googleapis.com/maps/api/js?sensor=false"></script>
    <%= stylesheet_link_tag "all" %>
    <%= javascript_include_tag "all" %>
  </head>

  <body class="container-fluid" ng-controller="ApplicationController"><%= partial 'app' %></body>
</html>
